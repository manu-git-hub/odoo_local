<odoo>
    <data>
        <record id="access_company_goal" model="ir.rule">
            <field name="name">Access Company Goal</field>
            <field name="model_id" ref="model_company_goal"/>
            <field name="groups" eval="[(4, ref('base.group_user'))]"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="False"/>
            <field name="perm_unlink" eval="False"/>
        </record>

        <record id="access_employee_goal" model="ir.rule">
            <field name="name">Access Employee Goal</field>
            <field name="model_id" ref="model_employee_goal"/>
            <field name="groups" eval="[(4, ref('base.group_user'))]"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="False"/>
            <field name="perm_unlink" eval="False"/>
        </record>
    </data>
</odoo>
